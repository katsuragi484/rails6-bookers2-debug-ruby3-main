<% if user_signed_in? %>
  <div>
    <%= form_with url: search_path, method: :get, local: true do |f| %>
      <%= f.text_field :keyword, value: @keyword %>
      <%= f.select :kind, {'Book': 'book' , 'User': 'user' } %>
      <%= f.select :match_type, {'完全一致': 'perfect_match' , '前方一致': 'fowward_match' ,'後方一致': 'backword_match', '部分一致': 'partial_match' } %>
      <%= f.submit "検索"%>
    <% end %>
  </div>
<% end %>